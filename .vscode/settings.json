{
	"editor.tabSize": 2,
	"editor.detectIndentation": false,
	"editor.insertSpaces": false,
	"files.trimTrailingWhitespace": true,
	"files.encoding": "utf8",
	"files.eol": "\n",
	"files.exclude": {
		"**/bin": true,
		"**/obj": true,
		".vscode": false,
		"**/.git": true,
		"**/project.lock.json": true
	},
	"search.exclude": {
		"**/bin": true,
		"**/obj": true,
		".vscode": true,
		"**/project.lock.json": true
	}
}
